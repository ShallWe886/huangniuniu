/*每个页面公共css */
.select_icon {
  display: inline-block;
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  border: 2rpx solid #cccccc;
}
.select_icon.right {
  position: relative;
  border: 2rpx solid #398EFC;
  background: #398EFC;
  transition: all 0.3s;
}
.select_icon.right::before {
  position: absolute;
  content: "";
  left: 6rpx;
  top: 22rpx;
  width: 10rpx;
  height: 4rpx;
  background: #ffffff;
  border-radius: 2rpx;
  -webkit-transform: rotate(45deg);
          transform: rotate(45deg);
}
.select_icon.right::after {
  position: absolute;
  content: "";
  left: 10rpx;
  top: 18rpx;
  width: 22rpx;
  height: 4rpx;
  background: #ffffff;
  border-radius: 2rpx;
  -webkit-transform: rotate(-45deg);
          transform: rotate(-45deg);
}
.select_icon.fault, .select_icon.disabled {
  position: relative;
  border: 2rpx solid #F4413B;
  background: #F4413B;
  transition: all 0.3s;
}
.select_icon.fault::before, .select_icon.disabled::before {
  position: absolute;
  content: "";
  left: 6rpx;
  top: 16rpx;
  width: 24rpx;
  height: 6rpx;
  background: #ffffff;
  border-radius: 3rpx;
  -webkit-transform: rotate(45deg);
          transform: rotate(45deg);
}
.select_icon.fault::after, .select_icon.disabled::after {
  position: absolute;
  content: "";
  left: 6rpx;
  top: 16rpx;
  width: 24rpx;
  height: 6rpx;
  background: #ffffff;
  border-radius: 3rpx;
  -webkit-transform: rotate(-45deg);
          transform: rotate(-45deg);
}
.select_icon.disabled {
  border: 2rpx solid #DDDDDD;
  background: #DDDDDD;
}
